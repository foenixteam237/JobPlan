<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JOBPLAN"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Composables.ui.UserProfile"
            android:exported="false"
            android:label="@string/title_activity_user_profile"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.Conversation"
            android:exported="false"
            android:label="@string/title_activity_conversation"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.ChatScreen"
            android:exported="false"
            android:label="@string/title_activity_chat_screen"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.PlanningSet"
            android:exported="false"
            android:label="@string/title_activity_planning_set"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.screens.OptionDisplay"
            android:exported="false"
            android:label="@string/title_activity_option_display"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.AdminDashBoad"
            android:exported="false"
            android:label="@string/title_activity_admin_dash_boad"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".RegisterPage"
            android:exported="false"
            android:label="@string/title_activity_register_page"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".Composables.ui.HomeScreen"
            android:exported="false"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/Theme.JOBPLAN" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JOBPLAN">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>